<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>ToolsPortable</AssemblyName>
    <RootNamespace>ToolsPortable</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>1.1.0.0</AssemblyVersion>
    <FileVersion>1.1.0.0</FileVersion>
    <Version>1.1</Version>
    <Authors>Bohdan Benetskyi</Authors>
    <Product>.NET Tools Portable</Product>
    <Copyright>Copyright Â©  2018</Copyright>
    <PackageProjectUrl>https://github.com/bbenetskyy/Tools</PackageProjectUrl>
    <RepositoryUrl>https://github.com/bbenetskyy/Tools</RepositoryUrl>
    <RepositoryType>GitHub Source</RepositoryType>
    <PackageTags>.Net Standard 2.0</PackageTags>
    <PackageReleaseNotes>Move from PCL to .Net Standard 2.0;
Added PropertyDescriptor extention;</PackageReleaseNotes>
    <NeutralLanguage />
    <Description>Tools library with many useful extensions for C# language, for quick import to any solution.</Description>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>NETSTANDARD2_0;NETSTANDARD2_0</DefineConstants>
  </PropertyGroup>

</Project>
